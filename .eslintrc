{
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "ecmaFeatures": {
    "modules": true
  },
  "rules": {
    "semi": 2,
    "no-console": 0,
    "no-unused-vars": 1,
    "no-var": 2,
    "constructor-super": 2,
    "arrow-parens": 2,
    "arrow-spacing": 2
  }
}
